ne":70,"column":17},"source":"projects/even-or-odds/actions/deck.js","original":{"line":44,"column":73}},{"generated":{"line":70,"column":22},"source":"projects/even-or-odds/actions/deck.js","original":{"line":44,"column":78}},{"name":"message","generated":{"line":70,"column":23},"source":"projects/even-or-odds/actions/deck.js","original":{"line":44,"column":79}},{"generated":{"line":71,"column":0},"source":"projects/even-or-odds/actions/deck.js","original":{"line":44,"column":33}},{"generated":{"line":71,"column":7},"source":"projects/even-or-odds/actions/deck.js","original":{"line":44,"column":32}},{"generated":{"line":71,"column":8},"source":"projects/even-or-odds/actions/deck.js","original":{"line":44,"column":20}},{"generated":{"line":72,"column":0},"source":"projects/even-or-odds/actions/deck.js","original":{"line":44,"column":20}},{"generated":{"line":72,"column":5},"source":"projects/even-or-odds/actions/deck.js","original":{"line":30,"column":11}},{"generated":{"line":72,"column":6},"source":"projects/even-or-odds/actions/deck.js","original":{"line":30,"column":4}},{"generated":{"line":73,"column":0},"source":"projects/even-or-odds/actions/deck.js","original":{"line":45,"column":1}},{"generated":{"line":73,"column":3},"source":"projects/even-or-odds/actions/deck.js","original":{"line":29,"column":36}},{"generated":{"line":74,"column":0},"source":"projects/even-or-odds/actions/deck.js","original":{"line":29,"column":36}},{"generated":{"line":74,"column":1},"source":"projects/even-or-odds/actions/deck.js","original":{"line":29,"column":7}}],"sources":{"projects/even-or-odds/actions/deck.js":"// Import Types\r\n// Each actions to be dispatch have to be accompany by 'type' \r\nimport { DECK, DECK_DRAW } from './types';\r\n\r\nexport const fetchDeckSuccess = deckJson => {\r\n    const { remaining, deck_id } = deckJson;\r\n\r\n    return { type: DECK.FETCH_SUCCESS, remaining, deck_id };\r\n}\r\n\r\nexport const fetchDeckError = error => {\r\n    return { type: DECK.FETCH_ERROR, message: error.message };\r\n}\r\n\r\n// Use double arrow functions so that thunk could work\r\nexport const fetchNewDeck = () => dispatch => {\r\n    return fetch('https://deckofcardsapi.com/api/deck/new/shuffle/?deck_count=1')\r\n        .then(response => { \r\n            if (response.status !== 200) {\r\n                throw new Error('Unsuccessful request to deckofcard api');\r\n            }\r\n            return response.json();\r\n        })\r\n        .then(json => dispatch(fetchDeckSuccess(json)))\r\n        .catch(error => dispatch(fetchDeckError(error)));\r\n}\r\n\r\n// Use double arrow functions so that thunk could work\r\nexport const fetchDrawCard = deck_id => dispatch => {\r\n    return fetch(`https://deckofcardsapi.com/api/deck/${deck_id}/draw/?count=1`)\r\n        .then(response => {\r\n            if (response.status !== 200) {\r\n                throw new Error('Unsuccessful reques to deck of card api');\r\n            }\r\n            return response.json();\r\n        })\r\n        .then(json => {\r\n            dispatch({\r\n                type: DECK_DRAW.FETCH_SUCCESS,\r\n                cards: json.cards,\r\n                remaining: json.remaining\r\n            });\r\n        })\r\n        .catch(error => dispatch({ type: DECK_DRAW.FETCH_ERROR, message: error.message }));\r\n}"},"lineCount":null}},"error":null,"hash":"69852b429adf712982300d94832c234e","cacheData":{"env":{}}}